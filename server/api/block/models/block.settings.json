{
  "kind": "collectionType",
  "connection": "default",
  "collectionName": "blocks",
  "info": {
    "name": "Block",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": false
  },
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "description": {
      "type": "text"
    },
    "blocks_category": {
      "via": "blocks",
      "model": "blocks-category"
    },
    "block_image": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    }
  }
}
